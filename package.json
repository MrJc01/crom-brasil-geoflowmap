{
  "name": "crom-geoflowmap",
  "private": false,
  "version": "1.0.3",
  "type": "module",
  "description": "React component for 3D geographic flow visualization",
  "keywords": [
    "react",
    "map",
    "deck.gl",
    "visualization",
    "geoflow",
    "3d",
    "maplibre"
  ],
  "author": "Crom Brasil",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crom-brasil/geoflowmap"
  },
  "main": "./dist/crom-geoflowmap.umd.js",
  "module": "./dist/crom-geoflowmap.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/crom-geoflowmap.es.js",
      "require": "./dist/crom-geoflowmap.umd.js"
    }
  },
  "files": [
    "dist",
    "dist-standalone"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --mode lib",
    "build:standalone": "vite build --config vite.config.standalone.ts",
    "build:all": "npm run build:lib && npm run build:standalone",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "@deck.gl/extensions": "^9.2.6",
    "@deck.gl/mapbox": "^9.2.6",
    "deck.gl": "^9.2.6",
    "maplibre-gl": "^5.16.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0"
  },
  "dependencies": {
    "framer-motion": "^12.29.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.563.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@deck.gl/extensions": "^9.2.6",
    "@deck.gl/mapbox": "^9.2.6",
    "deck.gl": "^9.2.6",
    "maplibre-gl": "^5.16.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.0"
  },
  "overrides": {
    "@babel/parser": "7.28.5"
  }
}
